# Authentication (required)
APP_USERNAME=admin
APP_PASSWORD=generator9097

# Vercel Blob Storage (required for production)
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token

# Vercel Edge Config (required for session/config management)
# EDGE_CONFIG is auto-injected when you connect Edge Config store
EDGE_CONFIG=your_edge_config_connection_string
# Get Edge Config ID from: Vercel Dashboard > Storage > Your Edge Config > Settings
EDGE_CONFIG_ID=your_edge_config_id
# Create API token at: https://vercel.com/account/tokens
VERCEL_API_TOKEN=your_vercel_api_token
# Optional: Only needed if using a team account
VERCEL_TEAM_ID=your_team_id

# Optional: Pre-configure API keys
GEMINI_API_KEY=your_gemini_key
OPENAI_API_KEY=your_openai_key
